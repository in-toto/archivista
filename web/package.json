{
  "name": "judge-platform",
  "version": "0.0.0",
  "private": true,
  "description": "judge-platform",
  "author": "TestifySec",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build-storybook": "build-storybook",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config codegen.yml",
    "convert-svgs": "node convert-svgs.js",
    "develop": "gatsby develop --S -H judge.testifysec.localhost --port 8077 --verbose",
    "git:push-me": "git config --global push.default current",
    "lhci": "lhci autorun",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "pre-commit": "lint-staged",
    "pre-push": "npx lint-staged && npm test",
    "prebuild-storybook": "export NODE_OPTIONS=--openssl-legacy-provider",
    "prestorybook": "export NODE_OPTIONS=--openssl-legacy-provider && npm run build-storybook",
    "serve": "gatsby serve",
    "start": "npm run start:local-proxy",
    "start:local-proxy": "gatsby develop --S -H judge.testifysec.localhost --port 8077 --verbose -o",
    "start:remote-proxy": "GATSBY_REMOTE_PROXY=true npm run start",
    "storybook": "start-storybook -p 6006",
    "style": "prettier --check .",
    "test": "jest .",
    "test-storybook": "test-storybook --coverage",
    "typecheck": "tsc --noEmit"
  },
  "overrides": {
    "moment": "2.29.4",
    "parse-path": "7.0.0",
    "parse-url": "8.1.0"
  },
